{
    /* B站海外CDN域名（除非你知道这是什么，否则请不要修改） */
    host: 'upos-hz-mirrorakam.akamaized.net',
    /* 代理服务器运行的端口 */
    port: 2689,
    /* 重新查找最优节点的间隔时间(s) */
    refreshInterval: 3600,

    /* 详细调试输出
     * 启用后将显示TCP和HTTP ping的详细测试过程
     * 包括每个IP的测试结果、延迟、丢包率、抖动等信息
     * 默认: false (仅显示简单输出)
     */
    verboseDebug: true,

    /* akamTester 自动IP发现集成 */
    akamTester: {
        /* 启用/禁用自动IP发现（默认禁用以保持向后兼容） */
        enabled: true,

        /* 运行akamTester的间隔时间(s)
         * 推荐值: 900 (15分钟), 1800 (30分钟), 3600 (1小时)
         */
        interval: 900,

        /* Python可执行文件路径
         * Windows: 'python' 或 'python3' 或 'C:\\Python312\\python.exe'
         * Linux/Mac: 'python3' 或 '/usr/bin/python3'
         * 注意：如果使用conda环境，请设置condaEnv而不是pythonPath
         */
        pythonPath: 'python',

        /* Conda环境名称（可选）
         * 如果设置了此项，将使用 'conda run -n <env> python' 来运行脚本
         * 示例: 'AKAMTester'
         * 留空或null表示不使用conda环境
         */
        condaEnv: 'AKAMTester',

        /* akamTester.py脚本路径（相对于项目根目录） */
        scriptPath: 'tools/akamTester/akamTester.py',

        /* 要测试的域名列表（akamTester支持多域名）
         * 目前只使用主域名以避免复杂性
         */
        targetHosts: ['upos-hz-mirrorakam.akamaized.net', 'upos-sz-mirroraliov.bilivideo.com'],

        /* 是否将合并后的IP列表保存回ip_list.txt？
         * 这会在服务器重启后保留新发现的IP
         */
        saveToFile: true,

        /* akamTester最大执行时间（毫秒）
         * 如果执行时间过长则终止进程
         */
        timeout: 600000,  // 10分钟

        /* IP池中保留的最大IP数量
         * 防止无限增长
         */
        maxIps: 200
    }
}